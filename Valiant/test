local library = loadstring(game:HttpGet('https://raw.githubusercontent.com/heatdeck123/UI-Lib/main/Valiant/source.lua'))()

local Window = library:CreateWindow("Name", "Version", 16593051889)
local Main = Window:CreateTab("Main")
local Page = Main:CreateFrame("Page 1")
local Stat = Window:CreateTab("mbm")
local st = Stat:CreateFrame("Page 2")

Button = Page:CreateButton("Button", "Description", function()
CreateNotification("Title", "Description", function(value)
if value == true then
   loadstring(game:HttpGet("https://raw.githubusercontent.com/m1M-Plqer819/any/main/skdpo.lua"))()
else
   loadstring(game:HttpGet("https://raw.githubusercontent.com/m1M-Plqer819/any/main/skdpo.lua"))()
end
end)
end)

Toggle = st:CreateToggle("Toggle", "Description", function(arg)
Toggle:UpdateToggle("New Title", "New Description")
print(arg)
end)


